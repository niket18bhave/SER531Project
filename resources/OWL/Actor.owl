    <!-- http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#hasActedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#hasActedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#hasActor"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#hasActedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#Actor"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#hasActedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/niketbhave/ontologies/2020/10/MovieOntology#Movie"/>
            </owl:Restriction>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#hasActor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#hasActor">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#hasActor"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/niketbhave/ontologies/2020/10/MovieOntology#Movie"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#hasActor"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#Actor"/>
            </owl:Restriction>
        </rdfs:range>
    </owl:ObjectProperty>
    



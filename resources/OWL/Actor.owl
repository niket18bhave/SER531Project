    <!-- http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#nm0000280 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#nm0000280">
        <rdf:type rdf:resource="http://www.semanticweb.org/niketbhave/ontologies/2020/10/ActorOntology#Actor"/>
        <ActorOntology:hasDateOfBirth rdf:datatype="http://www.w3.org/2001/XMLSchema#date">19784</ActorOntology:hasDateOfBirth>
